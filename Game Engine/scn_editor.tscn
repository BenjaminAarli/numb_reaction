script/source = "extends GridContainer

onready var daddy = $\"../../..\"

# all block loaders. 
var block     = preload(\"res://Cogs/cogBlank.tscn\")
var textblock = preload(\"res://Cogs/cogText.tscn\")
var treeblock = preload(\"res://Cogs/cogTree.tscn\")
var branchblock = preload(\"res://Cogs/cogBranch.tscn\")
var treeendblock 	= preload(\"res://Cogs/cogTreeEnd.tscn\")
var change_pathblock = preload(\"res://Cogs/cogChangePath.tscn\")

var statcheckblock 	= preload(\"res://Editor/ed_statcheck.tscn\")
var dicecheckblock 	= preload(\"res://Editor/ed_dicecheck.tscn\")
var check_successblock = preload(\"res://Editor/ed_check_success.tscn\")
var check_failureblock = preload(\"res://Editor/ed_check_failure.tscn\")
var check_passive 	= preload(\"res://Editor/ed_passive_check.tscn\")
var labelblock 		= preload(\"res://Editor/ed_label.tscn\")
var gotoblock  		= preload(\"res://Editor/ed_goto.tscn\")
var changebgblock 	= preload(\"res://Editor/ed_changebg.tscn\")

func _on_GridContainer_sort_children():
	check_list_sort()
	pass

func check_list_sort():
	var tree = []
	var branch = []
	
	for c in get_children():
		if c.type == Data.COG_TYPE_TREE_END:
			tree.pop_back()
		
		if tree.size() == 0:
			c.add_to_right = 0
		
		if tree.size() > 0:
			c.add_to_right = 8 * tree.size()
		
		if c.type == Data.COG_TYPE_TREE:
			tree.append(c)
		pass
	pass

var grab_pos = null

func on_child_grab(child):
	grab_pos = child.get_mid_pos()
	daddy.selected_child = child
	daddy.selected_position = child.get_index()
	pass

# Keep in mind, y position will go up. 
# ... and index with go down. 
func on_child_release(child):
	var moved_child = false
	var cpos = child.get_mid_pos()
	for c in get_children():
		var move = c.get_mid_pos()
		
		if cpos < grab_pos: # Goes up
			if cpos <= move:
				move_child(child, c.get_index())
				moved_child = true
				break
		if cpos > grab_pos: # Goes down
			if cpos < move:
				move_child(child, c.get_index() - 1)
				moved_child = true
				break
	
	if not moved_child and cpos < grab_pos: # goes up
		move_child(child, 0)
		child.set_moved_pos(0)
	if not moved_child and cpos > grab_pos: # goes down
		move_child(child, get_child_count() - 1)
		child.set_moved_pos(0)
		
	if moved_child:
		child.set_moved_pos(0)
	
	grab_pos = null
	
	check_list_sort()
	pass

var colr = [128, 128, 128]
var blocks_spawned = 0

func generate_cogwheel_using_file(file):
	for f in file:
		generate_block(f)
	daddy.emit_signal(\"file_loaded\")
	pass

func generate_block(data):
	var nc
	match data.type:
		Data.COG_TYPE_TEXT:
			nc = textblock.instance()
			nc.set_data(data)
		Data.COG_TYPE_TREE:
			nc = treeblock.instance()
			nc.set_data(data)
		Data.COG_TYPE_TREE_END:
			nc = treeendblock.instance()
		Data.COG_TYPE_BRANCH:
			nc = branchblock.instance()
			nc.set_data(data)
		Data.COG_TYPE_CHANGE_PATH:
			nc = change_pathblock.instance()
			nc.set_data(data)
		_: 
			return 0
	
	nc.connect(\"onRelease\", self, \"on_child_release\")
	nc.connect(\"onGrab\", self, \"on_child_grab\")
	
	add_child(nc)
	pass

func add_block(block_type):
	var nc = block_type.instance()
	nc.connect(\"onRelease\", self, \"on_child_release\")
	nc.connect(\"onGrab\", self, \"on_child_grab\")
	
	add_child(nc)
	blocks_spawned += 1
	pass

# Give each part that needs data, their data.
func update_each_part(data):
	
	for p in get_children():
		p.set_data(data)
		pass
	pass

func _on_AddBlock_pressed():
	add_block(block)
	pass

func _on_AddTextblock_pressed():
	add_block(textblock)
	pass

func _on_btn_tree_pressed():
	add_block(treeblock)
	pass

func _on_btn_path_pressed():
	add_block(branchblock)
	pass


func _on_btn_tree_end_pressed():
	add_block(treeendblock)
	pass


func _on_btn_dicecheck_pressed():
	add_block(dicecheckblock)
	pass 


func _on_btn_statcheck_pressed():
	add_block(statcheckblock)
	pass 


func _on_btn_passive_check_pressed():
	add_block(check_passive)
	pass


func _on_btn_check_success_pressed():
	add_block(check_successblock)
	pass 


func _on_btn_check_failure_pressed():
	add_block(check_failureblock)
	pass 


func _on_btn_label_pressed():
	add_block(labelblock)
	pass


func _on_btn_goto_pressed():
	add_block(gotoblock)
	pass 


func _on_btn_changebg_pressed():
	add_block(changebgblock)
	pass


func _on_btn_change_path_pressed():
	add_block(change_pathblock)
	pass


func _on_scn_editor_file_data_has_changed(data):
	update_each_part(data)
	pass

func _on_Paths_path_data_updated(data):
	pass # Replace with function body.
