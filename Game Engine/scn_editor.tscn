script/source = "extends Tabs

# Section PATHS

signal path_data_updated(data)

onready var List = $ItemList
onready var NewPathName = $NewPathName

onready var daddy = $\"../../..\"

var path = \"res://Data/editor/paths/\"

var current_path_name = null

func _ready():
	load_paths()
	is_editable()

func is_editable():
	pass

# Path Editing, Getting and Setting.

func add_path(text: String):
	List.add_item(text)
	NewPathName.text = \"\"
	emit_signal(\"path_data_updated\", get_all_paths())
	pass

func remove_path(index: int):
	if list_get_selected_item() != null:
		List.remove_item(index)
	emit_signal(\"path_data_updated\", get_all_paths())
	pass

func edit_path(index: int):
	if list_get_selected_item() != null:
		pass
	pass

# Path File getting.

func get_all_path_files():
	var files = []
	var dir = Directory.new()
	dir.open(path)
	dir.list_dir_begin()
	
	while true:
		var file = dir.get_next()
		if file == \"\":
			break
		elif not file.begins_with(\".\") and file.begins_with(\"path\"):
			files.append(path + file)
	
	dir.list_dir_end()
	return files

func get_all_paths():
	var files = get_all_path_files()
	
	var paths = {}
	
	for f in files:
		var pth = ResourceLoader.load(str(f))
		paths[pth.display_name] = pth
	return paths

# File Editing, Getting and Setting.

func remove_file(pname):
	var dir = Directory.new()
	dir.open(path)
	dir.list_dir_begin()
	while true:
		var file = dir.get_next()
		if file == \"\":
			break
		elif not file.begins_with(\".\") and file.begins_with(\"path\"):
			if file.begins_with(\"path_\" + pname):
				dir.remove(\"path_\" + pname + \".tres\")
				print(\"Path Removed: \", \"path_\" + pname)
			pass
	dir.list_dir_end()
	pass

# Save and Load Paths

func save_path(pth: numb_path):
	print(\"save_path(): PTH: \", pth.display_name)
	ResourceSaver.save(path + \"path_\" + pth.display_name + \".tres\", pth)
	pass

func load_paths(): # Load all paths.
	List.clear()
	for p in get_all_paths().values():
		List.add_item(str(p.display_name))
	pass

# Paths creation # 

# Keep in mind, we are not saving these; just making them. 
# To save them, use save_path(make_new_path(\"name of path\"))
func make_new_path(pname):
	var _path = numb_path.new()
	_path.story = [] # Empty because its new
	_path.display_name = str(pname)
	return _path

func make_path(pname, pdata):
	var _path = numb_path.new()
	_path.story = pdata # Empty because its new
	_path.display_name = str(pname)
	return _path

func make_current_path():
	var _path = numb_path.new()
	_path.story = daddy.get_current_path()
	_path.display_name = str(current_path_name)
	return _path

# List Functions

func list_get_selected_item():
	if List.get_selected_items()[-1] > -1:
		return List.get_selected_items()[-1]
	else:
		return null
	pass

func list_get_selected_text():
	return List.get_item_text(List.get_selected_items()[-1])

func list_get_selected_data():
	if list_get_selected_text() == null:
		return null
	else:
		return get_all_paths()[list_get_selected_text()]

func add_paths_to_list(arr):
	daddy.array_to_path(arr)
	pass

# SIGNALS # SIGNALS # SIGNALS # SIGNALS # SIGNALS # SIGNALS #
# ///////////////////////////////////////////////////////// #
# SIGNALS # SIGNALS # SIGNALS # SIGNALS # SIGNALS # SIGNALS #

func _on_NewPathName_text_changed(new_text):
	is_editable()
	pass

func _on_ItemList_item_selected(index):
	add_paths_to_list(list_get_selected_data()[\"story\"])
	current_path_name = list_get_selected_text()
	daddy.update_list_data()
	emit_signal(\"path_data_updated\", get_all_paths())
	pass

func _on_new_path_pressed():
	save_path(make_new_path(str(NewPathName.text)))
	add_path(str(NewPathName.text))
	daddy.update_list_data()
	emit_signal(\"path_data_updated\", get_all_paths())
	pass

func _on_edit_path_pressed():
	pass

func _on_remove_path_pressed():
	remove_file(list_get_selected_text())
	remove_path(list_get_selected_item())
	load_paths()
	emit_signal(\"path_data_updated\", get_all_paths())
	pass

func _on_savePath_pressed():
	save_path(make_current_path())
	emit_signal(\"path_data_updated\", get_all_paths())
	pass

func _on_Paths_path_data_updated(data):
	pass # Replace with function body.




































