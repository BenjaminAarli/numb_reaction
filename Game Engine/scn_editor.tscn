script/source = "extends Tabs

# Section CHARACTER CREATION

signal character_data_updated(data)

onready var list 		= $ItemList
onready var charName 	= $MakeAChar/charName
onready var description = $MakeAChar/description
onready var displayName = $MakeAChar/displayName
onready var portrait    = $TextureButton
onready var hasCharacterBeenEdited = $hasCharacterBeenEdited

onready var active_characters := get_node(\"active_characters_list\") as ItemList

onready var daddy = $\"../../..\"

var noPortraitTexture = preload(\"res://Data/images/portraits/unknown_man.png\")

var characters = {}

# Save Stuff
var save_path = \"res://Data/editor/characters/\"
var file_name = \"test.tres\"

# Portraits Location
var portrait_path = \"res://images/portraits/\"

func _ready():
	$FD_GetTexture.current_dir = portrait_path
	characters = get_all_characters()
	ui_set_character_in_list()
	update_data()
	pass 

func update_data():
	active_characters.items.clear()
	
	var _chars = get_all_characters(true) as Array
	var _active_chars_names = active_characters.items
	
	var ac = []
	
	for dn in _active_chars_names:
		if typeof(dn) == TYPE_STRING:
			if _chars.find(dn):
				ac.append(dn)
			pass
		if dn is numb_character:
			if _chars.find(dn):
				ac.append(dn)
			pass
	
	Data.ed_characters = ac
	pass

func is_current_character_saved():
	if get_selected_item() == null:
		return false
	
	var current_character_filedata = get_all_characters(true)[get_selected_item()]
	var current_character_editordata = get_editor_character_data()
	
	if current_character_filedata == current_character_editordata:
		hasCharacterBeenEdited.show()
		return true
	else:
		hasCharacterBeenEdited.hide()
		return false
	pass

func ui_set_character_in_list():
	list.clear()
	characters.clear()
	characters = get_all_characters()
	for c in characters:
		list.add_item(c)
	pass

func get_all_character_files():
	var files = []
	var dir = Directory.new()
	dir.open(save_path)
	dir.list_dir_begin()
	
	while true:
		var file = dir.get_next()
		if file == \"\":
			break
		elif not file.begins_with(\".\") and file.begins_with(\"chr\"):
			files.append(save_path + file)
	
	dir.list_dir_end()
	return files

func get_all_characters(useArray = false):
	var files = get_all_character_files()
	var _characters
	if useArray:
		_characters = []
		for f in files:
			var cha = ResourceLoader.load(str(f))
			_characters.append(cha)
	if not useArray:
		_characters = {}
		for f in files:
			var cha = ResourceLoader.load(str(f))
			_characters[cha.display_name] = cha
		
	return _characters

func add_char_to_list(cha):
	list.add_item(cha.display_name)
	characters[cha.display_name] = cha
	save_character_to_file(cha)
	ui_set_character_in_list()
	update_data()
	pass

func remove_char_from_list(index: int):
	list.remove_item(index)
	characters.erase(characters.get(index))
	ui_set_character_in_list()
	update_data()
	pass

func edit_char(index: int):
	var cha = characters[list.get_item_text(index)]
	
	charName.text 	 = cha.fname
	displayName.text = cha.display_name
	description.text = cha.description
	if cha.portrait != null:
		portrait.texture_normal = cha.portrait
	else:
		portrait.texture_normal = noPortraitTexture
	
	check_editability()
	update_data()
	pass

func text_entered_display_name(text: String):
	check_editability()
	pass

func check_editability():
	# CAN EDIT
	if displayName.text.length() != 0:
		charName.editable = true
		description.readonly = false
		portrait.disabled = false
	# CANNOT EDIT
	else: 
		charName.editable = false
		description.readonly = true
		portrait.disabled = true
		if portrait.texture_normal != load(\"res://images/portraits/border_transparent.png\"):
			portrait.texture_normal = load(\"res://images/portraits/border_transparent.png\")
	pass

func clear_edits():
	# Reset the name texts
	charName.text 		= \"\"
	displayName.text 	= \"\"
	description.text 	= \"\"
	portrait.texture_normal = load(\"res://images/portraits/Bearded Glasses Man.png\")
	pass

func get_selected_item():
	if list.get_selected_items().size() > 0:
		return list.get_selected_items()[-1]
	else:
		return null

func get_editor_character_data():
	var data = numb_character.new()
	data.fname 		  = str(charName.text)
	data.description  = str(description.text)
	data.display_name = str(displayName.text)
	data.portrait	  = portrait.texture_normal
	return data

func save_character_to_file(cha: numb_character):
	ResourceSaver.save(save_path + \"chr_\" + cha.display_name + \".tres\", cha)
	pass

func save_character_from_list():
	if list.get_selected_items().size() > 0:
		var selected = list.get_selected_items()[-1]
		ResourceSaver.save(save_path + \"chr_\" + characters[characters.keys()[selected]].display_name + \".tres\", characters[characters.keys()[selected]])
	pass

func save_characters_all():
	for c in characters.keys().size():
		ResourceSaver.save(save_path + \"chr_\" + characters[characters.keys()[c]].display_name + \".tres\", characters[characters.keys()[c]])
	pass

# SIGNALS # SIGNALS # SIGNALS #	# SIGNALS # SIGNALS # SIGNALS ## SIGNALS # SIGNALS # SIGNALS #
# /////////////////////////// # # /////////////////////////// ## /////////////////////////// # 
# SIGNALS # SIGNALS # SIGNALS # # SIGNALS # SIGNALS # SIGNALS ## SIGNALS # SIGNALS # SIGNALS #

# Button Signals

func _on_btnAdd_pressed():
	add_char_to_list(get_editor_character_data())
	clear_edits()
	ui_set_character_in_list()
	pass 

func _on_btnEdit_pressed():
	add_char_to_list(get_editor_character_data())
	ui_set_character_in_list()
	pass 

func _on_btnRemove_pressed():
	var dir = Directory.new()
	dir.remove(save_path + \"chr_\" + characters[characters.keys()[list.get_selected_items()[-1]]].display_name + \".tres\")
	remove_char_from_list(list.get_selected_items()[-1])
	ui_set_character_in_list()
	pass 

func _on_btnClear_pressed():
	clear_edits()
	list.unselect_all()
	ui_set_character_in_list()
	pass 

# Item List Signals

func _on_ItemList_item_selected(index):
	edit_char(index)
	daddy.update_list_data()
	pass

func _on_ItemList_nothing_selected():
	clear_edits()
	list.unselect_all()
	daddy.update_list_data()
	pass

# Characters start with \"chr_\"
func _on_btnSaveChar_pressed():
	save_character_from_list()
	ui_set_character_in_list()
	pass


func _on_btnSaveCharAll_pressed():
	save_characters_all()
	ui_set_character_in_list()
	pass

func _on_displayName_text_changed(new_text):
	text_entered_display_name(new_text)
	check_editability()
	is_current_character_saved()
	pass

func set_character_portrait(tex):
	portrait.texture_normal = tex
	check_editability()
	is_current_character_saved()
	pass

func _on_FD_GetTexture_file_selected(path):
	set_character_portrait(load(path))
	pass # Replace with function body.


func _on_TextureButton_pressed():
	$FD_GetTexture.popup_centered(Vector2(900, 600))
	pass # Replace with function body.


func _on_charName_text_changed(new_text):
	check_editability()
	is_current_character_saved()
	pass # Replace with function body.


func _on_description_text_changed():
	check_editability()
	is_current_character_saved()
	pass # Replace with function body.

# ACTIVE CHARACTERS SIGNALS

func _on_ItemList_item_activated(index):
	active_characters.add_item(list.get_item_text(index))
	pass # Replace with function body.
"

